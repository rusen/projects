<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout
android:id="@+id/widget35"
android:layout_width="fill_parent"
android:layout_height="fill_parent"
android:background="@color/arkafon"
xmlns:android="http://schemas.android.com/apk/res/android"
>
<TextView
android:id="@+id/widget47"
android:layout_width="320px"
android:layout_height="wrap_content"
android:text="@string/company_name"
android:textSize="20sp"
android:gravity="center"
android:textColor="@color/blue"
android:layout_x="0px"
android:layout_y="30px"
>
</TextView>
<Button
android:id="@+id/back"
android:layout_width="100px"
android:layout_height="40px"
android:text="Back"
android:textColor="@color/black"
android:textSize="18sp"
android:textStyle="italic"
android:layout_x="3px"
android:layout_y="100px"
>
</Button>
<Button
android:id="@+id/exit"
android:layout_width="82px"
android:layout_height="40px"
android:text="Exit"
android:textSize="14sp"
android:gravity="center"
android:textColor="@color/blue"
android:layout_x="119px"
android:layout_y="340px"
>
</Button>
<TextView
android:id="@+id/widget54"
android:layout_width="320px"
android:layout_height="wrap_content"
android:text="Kill Bill Software Company 2010"
android:gravity="center"
android:textSize="10sp"
android:textColor="@color/blue"
android:layout_x="0px"
android:layout_y="385px"
>
</TextView>
<TextView
android:id="@+id/widget55"
android:layout_width="320px"
android:layout_height="wrap_content"
android:text="All rights reserved."
android:gravity="center"
android:textSize="10sp"
android:textColor="@color/blue"
android:layout_x="0px"
android:layout_y="400px"
>
</TextView>
<EditText
android:id="@+id/text"
android:layout_width="200px"
android:layout_height="100px"
android:gravity="top"
android:text=""
android:textStyle="normal"
android:background="@color/metin"
android:textColor="@color/black"
android:layout_x="110px"
android:layout_y="103px"
>
</EditText>
<TextView
android:id="@+id/widget50"
android:layout_width="240px"
android:layout_height="wrap_content"
android:text="To:"
android:textSize="14sp"
android:gravity="left"
android:textStyle="bold"
android:textColor="@color/blue"
android:layout_x="20px"
android:layout_y="220px"
>
</TextView>
<EditText
android:id="@+id/receiverName"
android:layout_width="200px"
android:layout_height="20px"
android:text=""
android:textStyle="normal"
android:background="@color/metin"
android:textColor="@color/black"
android:layout_x="110px"
android:layout_y="220px"
>
</EditText>
<Button
android:id="@+id/send"
android:layout_width="wrap_content"
android:layout_height="40px"
android:text="Send"
android:textColor="@color/black"
android:textSize="18sp"
android:textStyle="italic"
android:layout_x="245px"
android:layout_y="250px"
>
</Button>
</AbsoluteLayout>