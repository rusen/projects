<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout
android:id="@+id/widget35"
android:layout_width="fill_parent"
android:layout_height="fill_parent"
android:background="@color/arkafon"
xmlns:android="http://schemas.android.com/apk/res/android"
>
<TextView
android:id="@+id/widget31"
android:layout_width="wrap_content"
android:layout_height="20px"
android:layout_marginLeft="40px"
android:text="Username"
android:textColor="@color/blue"
android:layout_x="81px"
android:layout_y="140px"
>
</TextView>
<EditText
android:id="@+id/password"
android:layout_width="90px"
android:layout_height="30px"
android:textSize="10sp"
android:password="true"
android:layout_x="155px"
android:layout_y="170px"
>
</EditText>
<EditText
android:id="@+id/userName"
android:layout_width="90px"
android:layout_height="30px"
android:textSize="10sp"
android:layout_x="155px"
android:layout_y="140px"
>
</EditText>
<TextView
android:id="@+id/widget41"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Password"
android:textColor="@color/blue"
android:layout_x="83px"
android:layout_y="170px"
>
</TextView>
<Button
android:id="@+id/widget45"
android:layout_width="wrap_content"
android:layout_height="40px"
android:text="LOGIN"
android:textStyle="bold"
android:textSize="14sp"
android:gravity="center"
android:textColor="@color/blue"
android:layout_x="155px"
android:layout_y="200px"
>
</Button>
<TextView
android:id="@+id/widget47"
android:layout_width="320px"
android:layout_height="wrap_content"
android:text="@string/company_name"
android:gravity="center"
android:textColor="@color/blue"
android:layout_x="0px"
android:layout_y="105px"
>
</TextView>
<Button
android:id="@+id/widget50"
android:layout_width="82px"
android:layout_height="40px"
android:text="Settings"
android:textColor="@color/blue"
android:textSize="14sp"
android:typeface="sans"
android:layout_x="62px"
android:layout_y="270px"
>
</Button>
<Button
android:id="@+id/widget51"
android:layout_width="82px"
android:layout_height="40px"
android:text="Exit"
android:textSize="14sp"
android:textColor="@color/blue"
android:layout_x="186px"
android:layout_y="270px"
>
</Button>
<TextView
android:id="@+id/widget52"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Mobile Solutions"
android:textColor="@color/blue"
android:layout_x="100px"
android:layout_y="30px"
>
</TextView>
<TextView
android:id="@+id/widget53"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="for Business"
android:textColor="@color/blue"
android:layout_x="100px"
android:layout_y="50px"
>
</TextView>
<TextView
android:id="@+id/widget54"
android:layout_width="320px"
android:layout_height="wrap_content"
android:text="Kill Bill Software Company 2010"
android:gravity="center"
android:textSize="10sp"
android:textColor="@color/blue"
android:layout_x="0px"
android:layout_y="385px"
>
</TextView>
<TextView
android:id="@+id/widget55"
android:layout_width="320px"
android:layout_height="wrap_content"
android:text="All rights reserved."
android:gravity="center"
android:textSize="10sp"
android:textColor="@color/blue"
android:layout_x="0px"
android:layout_y="400px"
>
</TextView>
<ImageView
android:id="@+id/widget28"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:src="@drawable/killbilllogo"
android:layout_x="85px"
android:layout_y="325px"
>
</ImageView>
<ImageView
android:id="@+id/widget27"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:src="@drawable/mobyui"
android:layout_x="23px"
android:layout_y="23px"
>
</ImageView>
</AbsoluteLayout>

